Proceso Actividad1_Nombre_Apellido
	//Realizar un programa el cual solicite su nombre y apellidos.
	Definir nombre Como Caracter;
	Definir apellido Como Caracter;
	
	Escribir "Escriba su Nombre completo por favor:";
	Leer nombre;
	Escribir "Escriba su Apellido completo por favor:";
	Leer apellido;
	
	Escribir "Su Nombre completo es:";
	Escribir nombre, " ", apellido;
FinProceso
